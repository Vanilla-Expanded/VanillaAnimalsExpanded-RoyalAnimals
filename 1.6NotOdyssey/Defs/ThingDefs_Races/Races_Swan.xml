			<MoveSpeed>2.75</MoveSpeed>
			<MarketValue>130</MarketValue>
			<LeatherAmount>36</LeatherAmount>
			<ComfyTemperatureMin>-10</ComfyTemperatureMin>
			<FilthRate>5</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>claws</label>
